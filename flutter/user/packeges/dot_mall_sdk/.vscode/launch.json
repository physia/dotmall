    "version": "0.2.0",
    "configurations": [
        {
            "name": "dot_mall_sdk",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "annotations",
            "cwd": "annotations",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "generators",
            "cwd": "generators",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Debug Generator",
            "request": "launch",
            "program": "example/.dart_tool/build/entrypoint/build.dart",
            "type": "dart",
            "args": ["build","--delete-conflicting-outputs"]
        },
        {
            "name": "Watch Debug Generator",
            "request": "launch",
            "program": "example/.dart_tool/build/entrypoint/build.dart",
            "type": "dart",
            "args": ["watch","--delete-conflicting-outputs"]
        } 
    ]
}